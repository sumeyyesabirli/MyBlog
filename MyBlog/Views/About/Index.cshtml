@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkında Veri Listesi</h2>
<table class=" table table-border">
    <tr>
        <th>ID</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif</th>
        <th>Pasif</th>
    </tr>
    @foreach(var item in Model)
     {
        <tr>
            <td>@item.AboutID</td>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-warning">Aktif</a></td>
            <td><a href="#" class="btn btn-danger">Pasif</a></td>
        </tr>

     }
</table>

@Html.Action("AboutPartial","About") 
     

